<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainMenu">

    <fragment
        android:id="@+id/mainMenu"
        android:name="com.powermango.myapplication.menuFragments.MainMenu"
        android:label="fragment_main_menu"
        tools:layout="@layout/fragment_main_menu">
        <action
            android:id="@+id/action_mainMenu_to_leccionesFrag"
            app:destination="@id/leccionesFrag" />
        <action
            android:id="@+id/action_mainMenu_to_menuEjercicios"
            app:destination="@id/menuEjercicios" />
    </fragment>
    <fragment
        android:id="@+id/leccionesFrag"
        android:name="com.powermango.myapplication.menuFragments.leccionesFrag"
        android:label="fragment_lecciones"
        tools:layout="@layout/fragment_lecciones">
        <action
            android:id="@+id/action_leccionesFrag_to_leccionDescripcion"
            app:destination="@id/leccionDescripcion" />
        <action
            android:id="@+id/action_leccionesFrag_to_diptongoHiatoFragment"
            app:destination="@id/diptongoHiatoFragment" />
        <action
            android:id="@+id/action_leccionesFrag_to_reglaGeneralFragment"
            app:destination="@id/reglaGeneralFragment" />
        <action
            android:id="@+id/action_leccionesFrag_to_casosEspecialesFragment"
            app:destination="@id/casosEspecialesFragment" />
    </fragment>
    <fragment
        android:id="@+id/leccionDescripcion"
        android:name="com.powermango.myapplication.menuFragments.leccionDescripcion"
        android:label="fragment_leccion_descripcion"
        tools:layout="@layout/fragment_leccion_descripcion" />
    <fragment
        android:id="@+id/menuEjercicios"
        android:name="com.powermango.myapplication.menuFragments.menuEjercicios"
        android:label="fragment_menu_ejercicios_frag"
        tools:layout="@layout/fragment_menu_ejercicios_frag">
        <action
            android:id="@+id/action_menuEjercicios_to_generalDefiniciones1"
            app:destination="@id/generalDefiniciones1" />
        <action
            android:id="@+id/action_menuEjercicios_to_generalDefiniciones22"
            app:destination="@id/generalDefiniciones2" />
        <action
            android:id="@+id/action_menuEjercicios_to_generalCategorias"
            app:destination="@id/generalCategorias" />
    </fragment>
    <fragment
        android:id="@+id/generalDefiniciones1"
        android:name="com.powermango.myapplication.exercisesFragments.EjercicioGeneralDefiniciones1Fragment"
        android:label="fragment_general_definiciones1"
        tools:layout="@layout/fragment_ejercicio_general_definiciones1" />
    <fragment
        android:id="@+id/generalDefiniciones2"
        android:name="com.powermango.myapplication.exercisesFragments.EjercicioGeneralDefiniciones2Fragment"
        android:label="fragment_general_definiciones2"
        tools:layout="@layout/fragment_ejercicio_general_definiciones2" />
    <fragment
        android:id="@+id/generalCategorias"
        android:name="com.powermango.myapplication.exercisesFragments.EjercicioGeneralCategoriasFragment"
        android:label="fragment_general_categorias"
        tools:layout="@layout/fragment_ejercicio_general_categorias" />
    <fragment
        android:id="@+id/reglaGeneralFragment"
        android:name="com.powermango.myapplication.lessonsFragments.ReglaGeneralFragment"
        android:label="fragment_regla_general"
        tools:layout="@layout/fragment_regla_general" />
    <fragment
        android:id="@+id/diptongoHiatoFragment"
        android:name="com.powermango.myapplication.lessonsFragments.DiptongoHiatoFragment"
        android:label="fragment_diptongo_hiato"
        tools:layout="@layout/fragment_diptongo_hiato" />
    <fragment
        android:id="@+id/casosEspecialesFragment"
        android:name="com.powermango.myapplication.lessonsFragments.CasosEspecialesFragment"
        android:label="fragment_casos_especiales"
        tools:layout="@layout/fragment_casos_especiales" />
</navigation>